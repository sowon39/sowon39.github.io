  "data": {
    "workout": {
      "1": {
        "day": "월요일",
        "type": "push",
        "title": "가슴 & 삼두 (Strength)",
        "list": [
          {
            "name": "체스트 딥스",
            "set": 4,
            "rep": 10,
            "rest": 90,
            "breath": "내려가며 흡기 / 가슴으로 밀어 올리며 호기",
            "target": "가슴 하부",
            "mind": "내려갈 때 3초를 세며 가슴 근육을 최대로 늘려라",
            "tip": "HOW\n1) 상체 15도 숙여 가슴 열기\n2) 3초 버티며 하강\n3) 폭발적으로 밀되 팔은 95%만 펴기",
            "guide": "UPGRADE\n- 네거티브 컨트롤이 가슴 모양을 만듭니다.",
            "cue": "3초 하강 + 가슴으로 밀어라"
          },
          {
            "name": "스탠다드 푸쉬업",
            "set": 4,
            "rep": 15,
            "rest": 60,
            "breath": "내려가며 흡기 / 밀며 호기",
            "target": "가슴 전체",
            "mind": "가슴 안쪽까지 쥐어짜듯 밀어낸다",
            "tip": "HOW\n1) 몸 일직선 유지\n2) 가슴 바닥 직전까지 하강\n3) 상단에서 가슴 수축",
            "guide": "MISTAKE\n- 엉덩이 처짐 주의",
            "cue": "몸 일직선 + 가슴 수축"
          },
          {
            "name": "내로우 딥스",
            "set": 3,
            "rep": 12,
            "rest": 60,
            "breath": "밀 때 호기",
            "target": "삼두근",
            "mind": "삼두 힘으로만 수직 이동",
            "tip": "HOW\n1) 상체 수직\n2) 팔꿈치 몸통 밀착\n3) 삼두 잠그듯 밀기",
            "guide": "CHECK\n- 상체 숙이면 가슴 개입",
            "cue": "팔꿈치 고정 + 삼두 잠금"
          },
          {
            "name": "캡틴스 체어 레그 레이즈",
            "set": 3,
            "rep": 10,
            "rest": 60,
            "breath": "올릴 때 호기 / 내릴 때 흡기",
            "target": "하복부",
            "mind": "다리가 아니라 골반을 말아 올린다",
            "tip": "HOW\n1) 어깨 내리고 몸 고정\n2) 골반 말며 다리 들어올리기\n3) 내릴 때 2초 버티기",
            "guide": "MISTAKE\n- 반동 사용 금지",
            "cue": "골반 말아서 올려라"
          }
        ]
      },

      "2": {
        "day": "화요일",
        "type": "pull",
        "title": "등 두께 (Vertical Focus)",
        "list": [
          {
            "name": "풀업 (밴드)",
            "set": 4,
            "rep": 8,
            "rest": 120,
            "breath": "당길 때 호기",
            "target": "광배 바깥",
            "mind": "내 힘으로 통제하며 수행",
            "tip": "HOW\n1) 숄더패킹\n2) 팔꿈치 아래로 찍기\n3) 하강 2초",
            "guide": "UPGRADE\n- 밴드 점진 제거",
            "cue": "팔꿈치 아래로 찍어라"
          },
          {
            "name": "밴드 로우 (치닝디핑)",
            "set": 4,
            "rep": 15,
            "rest": 75,
            "breath": "당길 때 호기",
            "target": "등 중앙",
            "mind": "날개뼈를 척추로 모은다",
            "tip": "HOW\n1) 밴드 팽팽하게\n2) 가슴 펴고 팔꿈치 뒤로\n3) 정점 1초",
            "guide": "ADVANTAGE\n- 가동범위 넓음",
            "cue": "팔꿈치 뒤로 길게"
          },
          {
            "name": "친업 (밴드)",
            "set": 3,
            "rep": 8,
            "rest": 90,
            "breath": "올라갈 때 호기",
            "target": "등 하부 & 이두",
            "mind": "이두를 써서 등을 더 수축",
            "tip": "HOW\n1) 언더그립\n2) 가슴을 봉으로\n3) 하강 컨트롤",
            "guide": "CHECK\n- 턱만 넘기지 말 것",
            "cue": "가슴을 봉으로"
          }
        ]
      },

      "3": {
        "day": "수요일",
        "type": "leg",
        "title": "하체 & 측면 어깨 (Foundation)",
        "list": [
          {
            "name": "스쿼트",
            "set": 5,
            "rep": 15,
            "rest": 90,
            "breath": "내려가며 흡기 / 밀며 호기",
            "target": "허벅지/둔근",
            "mind": "발바닥으로 지면을 민다",
            "tip": "HOW\n1) 고관절 먼저 접기\n2) 무릎 발끝 방향\n3) 복압 유지",
            "guide": "CHANGE\n- 하체 핵심 데이",
            "cue": "발바닥으로 지구 밀기"
          },
          {
            "name": "런지",
            "set": 4,
            "rep": 12,
            "rest": 60,
            "breath": "올라오며 호기",
            "target": "엉덩이",
            "mind": "뒤꿈치 80%",
            "tip": "HOW\n1) 상체 살짝 숙임\n2) 수직 하강\n3) 뒤꿈치로 밀기",
            "guide": "POINT\n- 무릎 방향 주의",
            "cue": "뒤꿈치 밀기"
          },
          {
            "name": "사이드 레이즈",
            "set": 5,
            "rep": 20,
            "rest": 60,
            "breath": "올릴 때 호기",
            "target": "측면 삼각근",
            "mind": "팔꿈치를 던진다",
            "tip": "HOW\n1) 승모 억제\n2) 옆으로 멀리\n3) 저항 하강",
            "guide": "UPGRADE\n- 고반복 펌핑",
            "cue": "옆으로 던져라"
          }
        ]
      },

      "4": {
        "day": "목요일",
        "type": "push",
        "title": "어깨 & 가슴 상부",
        "list": [
          {
            "name": "파이크 푸쉬업",
            "set": 4,
            "rep": 10,
            "rest": 90,
            "breath": "밀 때 호기",
            "target": "전면 어깨",
            "mind": "수직 프레스",
            "tip": "HOW\n1) 역V자\n2) 정수리 하강\n3) 어깨로 밀기",
            "guide": "MISTAKE\n- 팔꿈치 벌어짐 주의",
            "cue": "어깨로 밀어라"
          },
          {
            "name": "디클라인 푸쉬업",
            "set": 4,
            "rep": 12,
            "rest": 90,
            "breath": "밀 때 호기",
            "target": "상부 가슴",
            "mind": "쇄골 아래로 밀기",
            "tip": "HOW\n1) 발 높임\n2) 몸 일직선\n3) 윗가슴 수축",
            "guide": "MISTAKE\n- 어깨 통증 시 월요일로 이동",
            "cue": "윗가슴으로 밀어라"
          },
          {
            "name": "삼두 딥스",
            "set": 3,
            "rep": 12,
            "rest": 60,
            "breath": "밀 때 호기",
            "target": "삼두",
            "mind": "마무리 펌핑",
            "tip": "HOW\n1) 팔꿈치 고정\n2) 수축 1초",
            "guide": "TIP\n- 깊이 과욕 금지",
            "cue": "삼두 쥐어짜기"
          },
          {
            "name": "캡틴스 체어 레그 레이즈",
            "set": 3,
            "rep": 10,
            "rest": 60,
            "breath": "올릴 때 호기",
            "target": "하복부",
            "mind": "골반 말기",
            "tip": "HOW\n1) 반동 제거\n2) 하강 2초",
            "guide": "POINT\n- 코어로 상체 안정",
            "cue": "골반 말기"
          }
        ]
      },

      "5": {
        "day": "금요일",
        "type": "pull",
        "title": "등 넓이 (Width Focus)",
        "list": [
          {
            "name": "와이드 풀업",
            "set": 4,
            "rep": 6,
            "rest": 120,
            "breath": "올라갈 때 호기",
            "target": "광배 외곽",
            "mind": "등을 펼친다",
            "tip": "HOW\n1) 넓은 그립\n2) 팔꿈치 하강",
            "guide": "CHECK\n- 반동 금지",
            "cue": "등 펼치기"
          },
          {
            "name": "밴드 로우 (치닝디핑)",
            "set": 4,
            "rep": 12,
            "rest": 90,
            "breath": "당길 때 호기",
            "target": "등 전체",
            "mind": "코어 고정",
            "tip": "HOW\n1) 1초 정지\n2) 하강 컨트롤",
            "guide": "UPGRADE\n- 안정성 강화",
            "cue": "날개뼈 수축"
          },
          {
            "name": "내로우 풀업",
            "set": 3,
            "rep": 8,
            "rest": 90,
            "breath": "올라갈 때 호기",
            "target": "등 중앙",
            "mind": "마무리 집중",
            "tip": "HOW\n1) 가슴 봉 접근\n2) 1초 정지",
            "guide": "TIP\n- 마지막 자극",
            "cue": "가슴 붙이기"
          }
        ]
      },

      "6": {
        "day": "토요일",
        "type": "leg",
        "title": "하체 & 코어 (Conditioning)",
        "list": [
          {
            "name": "스쿼트",
            "set": 4,
            "rep": 20,
            "rest": 60,
            "breath": "일정",
            "target": "하체 전체",
            "mind": "끊김 없이",
            "tip": "HOW\n1) 큰 가동범위\n2) 리드미컬",
            "guide": "POINT\n- 지구력",
            "cue": "기계처럼"
          },
          {
            "name": "런지",
            "set": 3,
            "rep": 15,
            "rest": 60,
            "breath": "올라오며 호기",
            "target": "하체",
            "mind": "버티기",
            "tip": "HOW\n1) 균형 유지",
            "guide": "CHECK\n- 코어 흔들림",
            "cue": "둔근 조이기"
          },
          {
            "name": "플랭크",
            "set": 3,
            "rep": 60,
            "rest": 60,
            "breath": "짧고 일정",
            "target": "전신 코어",
            "mind": "판자",
            "tip": "HOW\n1) 배 당김",
            "guide": "MISTAKE\n- 허리 꺾임",
            "cue": "강철 일직선"
          }
        ]
      }
    },

